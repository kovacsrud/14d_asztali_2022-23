<Window x:Class="WpfOsztas.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfOsztas"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <TextBox x:Name="ertek_a" Width="200" Height="50" FontSize="30"/>
        <TextBox x:Name="ertek_b" Width="200" Height="50" FontSize="30" Grid.Row="1"/>
        <TextBlock x:Name="eredmeny" FontSize="30" Width="400" Height="50" Grid.Row="2" />
        <Button x:Name="buttonOsztas" Content="Osztás" Grid.Row="3" Width="100" Height="50" FontSize="25" Click="buttonOsztas_Click" />

    </Grid>
</Window>
